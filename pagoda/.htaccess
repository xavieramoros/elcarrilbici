# BEGIN WordPress
<IfModule mod_rewrite.c>
RewriteEngine On
RewriteBase /

<if "%{HTTP_HOST} == 'dev.example.com'">
# RewriteBase /elcarrilbici/
RewriteRule ^index\.php$ - [L]
RewriteCond %{REQUEST_FILENAME} !-f
RewriteCond %{REQUEST_FILENAME} !-d
# RewriteRule . /elcarrilbici/index.php [L]
RewriteRule . /index.php [L]
</IfModule>
# END WordPress
</if>

